package com.hms.userms.service;

import com.hms.userms.dto.UpdateUserRequest;
import com.hms.userms.entity.User;
import com.hms.userms.exception.UserNotFoundException;

public interface IUserService {

	User updateUser(UpdateUserRequest updateUserRequest) throws UserNotFoundException;

	User getUserByEmail(String email) throws UserNotFoundException;

	User addReferenceId(String email, String referenceId) throws UserNotFoundException;

}
