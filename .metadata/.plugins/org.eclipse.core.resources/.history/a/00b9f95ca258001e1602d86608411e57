package com.hms.bookingms.exceptions;

import java.util.Date;

public class InvalidDatesException extends RuntimeException {

	private static final long serialVersionUID = -7729423867105584603L;

	public InvalidDatesException(Date occupyDate, Date releaseDate) {
		super("Occupied date should be after booking date and release date should be after occupied date"
				+ "\nBooking Date : " + bookingDate + "\nOccupied Date : " + occupyDate + "\nRelease Date : "
				+ releaseDate);
	}

}
