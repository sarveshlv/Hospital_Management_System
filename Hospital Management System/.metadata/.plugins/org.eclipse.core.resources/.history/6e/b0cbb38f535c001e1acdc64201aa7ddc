package com.hms.authms.dto;

import lombok.Data;

@Data
public class JwtTokenResponse {
	private String token;
	private String type = "Bearer";
	private String email;
	private String role;

	public JwtTokenResponse(String token, String email, String role) {
		this.token = token;
		this.email = email;
		this.role = role;
	}
}
